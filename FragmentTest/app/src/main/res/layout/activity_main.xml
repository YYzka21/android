<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <fragment
        android:id="@+id/fragment_second"
        android:name="com.example.asus.fragmenttest.SecondFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="10"
        >
    </fragment>
    <FrameLayout
        android:id="@+id/third_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
    >
        <fragment
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:name="com.example.asus.fragmenttest.FragmentClick"
            android:id="@+id/click_fragment"></fragment>
        <fragment
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:name="com.example.asus.fragmenttest.ThirdFragment"
            android:id="@+id/fragment_third"></fragment>

    </FrameLayout>
    </LinearLayout>

